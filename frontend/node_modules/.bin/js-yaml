#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/js-yaml/bin/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/js-yaml/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/node_modules:/Users/itaygamliel/Desktop/node_modules:/Users/itaygamliel/node_modules:/Users/node_modules:/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/js-yaml/bin/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/js-yaml/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/node_modules:/Users/itaygamliel/Desktop/node_modules:/Users/itaygamliel/node_modules:/Users/node_modules:/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
