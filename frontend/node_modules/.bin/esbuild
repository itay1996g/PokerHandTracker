else
  export NODE_PATH="/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/esbuild/bin/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/esbuild/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/node_modules:/Users/itaygamliel/Desktop/node_modules:/Users/itaygamliel/node_modules:/Users/node_modules:/node_modules:/Users/itaygamliel/Desktop/PokerHandTracker/PokerHandTrack/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
